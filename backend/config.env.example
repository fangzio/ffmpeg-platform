# 七牛云存储配置文件
# 请将此文件复制为 config.env 并填入实际的配置信息

# 存储类型：local（本地存储）或 qiniu（七牛云存储）
STORAGE_TYPE=qiniu

# 七牛云配置
QINIU_ENABLED=true
QINIU_ACCESS_KEY=ak
QINIU_SECRET_KEY=sk
QINIU_BUCKET=fangziol
QINIU_DOMAIN=images.fangziol.cn
# 区域代码：z0=华东, z1=华北, z2=华南, na0=北美, as0=东南亚
QINIU_REGION=z2

# 本地临时目录（用于文件中转）
UPLOAD_DIR=./storage/uploads
OUTPUT_DIR=./storage/outputs

# 服务器配置
SERVER_PORT=8008

# 数据库配置
DATABASE_DSN=host=localhost user=ffmpeg password=ffmpeg dbname=ffmpeg port=5432 sslmode=disable

# Redis配置
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=

# FFmpeg配置
FFMPEG_PATH=ffmpeg
FFMPEG_LOG_LEVEL=info
